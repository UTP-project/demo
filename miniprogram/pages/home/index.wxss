/* miniprogram/pages/home/index.wxss */

.map-container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.map {
  width: 100%;
  height: 100%;
}

.cover-view {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
}

.cover-container {
  background: #fff;
  height: 180px;
  border-radius: 16px 16px 0 0;
  padding: 0 24px;
  overflow-y: scroll;
}

.btn {
  margin: 36px 24px;
  color: #fff;
  background-color: #00B2B2;
}